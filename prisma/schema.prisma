// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = env("DATABASE_URL")
}

model Burn {
  id    Int     @id @default(autoincrement())
  minted Int
  chain Int
  blockNumber Int?
  timestamp DateTime
  transactionHash String?
  transaction_id Int?
  tokenAddress String
  tokenId Int  @unique
  fromAddress String?
  toAddress String
}

model User {
  id    Int     @id @default(autoincrement())
  email String  @unique
  name  String?
}
